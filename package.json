{
  "name": "dlm-starwars-names",
  "version": "0.0.0-development",
  "description": "Get random Star Wars names",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "watch:test": "npm t -- -w",
    "test": "mocha src/index.test.js --compilers js:babel-register",
    "cover": "nyc npm t",
    "prebuild": "rimraf dist",
    "build": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "precommit": "npm run cover && npm run check-coverage",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DakotaLMartinez/starwars-names.git"
  },
  "keywords": [
    "random",
    "star",
    "wars"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Dakota Lee Martinez <dakotaleewebdev@gmail.com> (http://becomingaprogrammer.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DakotaLMartinez/starwars-names/issues"
  },
  "homepage": "https://github.com/DakotaLMartinez/starwars-names#readme",
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.24.1",
    "chai": "3.5.0",
    "codecov.io": "0.1.6",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.0.0",
    "husky": "0.13.3",
    "istanbul": "0.4.5",
    "mocha": "3.3.0",
    "nyc": "10.3.2",
    "rimraf": "2.6.1",
    "semantic-release": "^6.3.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
